import pandas as pd

# Read the first Excel file
df1 = pd.read_excel('file1.xlsx')

# Read the second Excel file
df2 = pd.read_excel('file2.xlsx')

# Compare df1 and df2 and get unmatched rows from df1
df_unmatched = df1.merge(df2, indicator=True, how='left').loc[lambda x: x['_merge'] == 'left_only'].drop(columns='_merge')

# Display unmatched rows from df1
print("Unmatched rows in df1:")
print(df_unmatched)
