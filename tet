import csv

# Sample data as a string
data = '''"hi","hello","hey\n/hey\n//hey"
"hii","hellol","hi\n/hi\n//hiiii"'''

# Process the data
def process_data(data):
    # Read data into a list
    reader = csv.reader(data.splitlines())
    
    # Prepare processed data
    processed_records = []
    
    for row in reader:
        # Remove newline characters from each field
        processed_row = [field.replace('\n', '').replace('\r', '') for field in row]
        processed_records.append(processed_row)
    
    return processed_records

# Process and display the data
processed_data = process_data(data)
for record in processed_data:
    print(record)
