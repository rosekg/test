#!/bin/bash

# Define the JAR file name pattern to match
JAR_NAME="0-snapshot.jar"

# Get the process ID (PID) of the Java process running the specified JAR file
PID=$(ps aux | grep "$JAR_NAME" | grep -v grep | awk '{print $2}')

# Check if PID is not empty and kill the process
if [ -n "$PID" ]; then
    echo "Killing process with PID $PID"
    kill -9 $PID
else
    echo "No process running with JAR name $JAR_NAME"
fi
