from jira import JIRA

# Set your Jira Cloud credentials and Jira URL
jira_url = 'your_jira_url'
username = 'your_username'
api_token = 'your_api_token'

# Authenticate with Jira Cloud
jira = JIRA(jira_url, basic_auth=(username, api_token))

# Set the issue key of the ticket you want to resolve
issue_key = 'your_issue_key'

# Get the issue
issue = jira.issue(issue_key)

# Resolve the issue
jira.transition_issue(issue, '5')  # Assuming '5' is the ID for the 'Resolved' status

print(f"Issue {issue_key} has been resolved.")
