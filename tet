git filter-repo --commit-callback '
if commit.committer_date[:10] == b"2023-12-21" or commit.original_id in (b"gghh34", b"obsd54"):
    commit.message = b""
    commit.file_changes = [change for change in commit.file_changes if change.path != b"deploy.sh"]
'






git clone <repository-url>
cd <repository>


git filter-repo --path <file-or-directory> --commit-callback '
    if commit.committer_date < b"2023-01-01T00:00:00":
        commit.message = b""
'
